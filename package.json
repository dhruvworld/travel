{
  "name": "travel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma": "prisma",
    "clean": "rm -rf .next out .netlify/cache",
    "validate": "next lint && prisma validate && tsc --noEmit",
    "postinstall": "prisma generate",
    "save:safe": "npm run validate && git add . && git commit -m 'safe save'",
    "fix": "next lint --fix && tsc --noEmit",
    "reset": "rm -rf node_modules .next && npm install"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.0",
    "@prisma/client": "^5.10.0",
    "@tailwindcss/forms": "^0.5.7",
    "@types/react-datepicker": "^6.0.1",
    "autoprefixer": "^10.4.17",
    "bcryptjs": "^3.0.2",
    "framer-motion": "^11.0.8",
    "next": "14.2.26",
    "next-auth": "^4.24.6",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-datepicker": "^6.1.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.50.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.0.0",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@types/node": "20.17.28",
    "@types/react": "18.3.20",
    "@types/react-dom": "^18.2.0",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.26",
    "postcss-import": "^16.0.1",
    "prisma": "^5.10.0",
    "typescript": "^5.3.3",
    "@types/autoprefixer": "^10.2.0",
    "@types/postcss-import": "^14.0.3"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
